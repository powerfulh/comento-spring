<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.comento.oracleSpringBoot.mapper.CurtainMapper">
	<select id="select100" resultType="map">call blind100()</select>
	<select id="select100comp" resultType="map" parameterType="string">call blind100company(#{company})</select>
